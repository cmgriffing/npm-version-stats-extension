{
  "name": "npm-version-stats-extension",
  "description": "Browser extension that shows npm package version usage statistics inline on the npmjs.com package page in the versions tab",
  "private": true,
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "pnpm run \"/^build:.*/\"",
    "build:chrome": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "pnpm run \"/^zip:.*/\"",
    "zip:firefox": "wxt zip -b firefox",
    "zip:chrome": "wxt zip",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@wxt-dev/module-react": "^1.1.3",
    "typescript": "^5.9.2",
    "wxt": "^0.20.6"
  }
}
